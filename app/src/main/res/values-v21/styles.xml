<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ThemeYellow" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/yellow</item>
        <item name="colorPrimaryDark">@color/yellow_dark</item>
        <item name="colorAccent">@color/blue</item>
        <item name="tintFab">@android:color/white</item>
        <item name="tintMenu">@color/tintMenu_Y</item>
        <item name="colorBackgroundLighter">@color/colorBackgroundLighter_Y</item>
        <item name="yellow_selector">@drawable/yellow_selector_y</item>
        <item name="icon_selector">@color/icon_selector_y</item>
        <item name="text_selector">@color/text_selector_y</item>
        <item name="divider">@drawable/divider_y</item>
        <item name="android:alertDialogTheme">@style/ThemeYellow.AlertDialog</item>
    </style>

    <style name="ThemeYellow.AlertDialog" parent="android:Theme.Material.Light.Dialog.Alert">
        <item name="android:colorPrimary">@color/yellow</item>
        <item name="android:colorPrimaryDark">@color/yellow_dark</item>
        <item name="android:colorAccent">@color/blue</item>
    </style>
</resources>